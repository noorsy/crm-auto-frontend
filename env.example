# CRM Auto Frontend Environment Configuration
# Copy this file to .env.local and update with your actual values

# Backend API URL - Update this with your actual backend URL
REACT_APP_API_URL=https://your-backend-repl-name.your-username.repl.co/api

# Example URLs for different deployment scenarios:
# Local development: http://localhost:5000/api
# Replit backend: https://your-backend-repl-name.your-username.repl.co/api
# Custom domain: https://api.yourcrm.com/api

# Application Settings
REACT_APP_NAME=CRM Auto Frontend
REACT_APP_VERSION=1.0.0

# Optional: Enable debug mode
REACT_APP_DEBUG=false 